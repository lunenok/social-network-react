tus-hooks';\nimport {Posts} from \"./post\";\nimport {Loader} from './loader/loader';\n\nconst ProfileInformation = ({currentProfile, status, updateProfileStatus}) => {\n\n    return (\n        <div className=\"profile\">\n            <div className=\"profile__pic \">\n                <img\n                    src={currentProfile.photos.large ? currentProfile.photos.large : \"https://freesvg.org/img/Linux-Avatar.png\"}\n                    alt=\"avatar\"\n                    width=\"{150}\"\n                    height=\"{150}\"\n                    className=\"profile__img\"\n                />\n            </div>\n            <div className=\"profile__info\">\n                <div className=\"profile__name\">\n                    {currentProfile.fullName}\n                </div>\n                <ProfileStatusWithHook status={status} updateProfileStatus={updateProfileStatus}/>\n                <div className=\"profile__description\">\n                    Date of birth: 10 january\n                </div>\n                <div className=\"profile__description\">\n                    City: Yekaterinburg\n                </div>\n                <div className=\"profile__description\">\n                    URFU'15\n                </div>\n                <div className=\"profile__description\">\n                    Web-site: none\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport const Profile = (props) => {\n    const {posts, newPostText, onUpdatePostText, addPost, currentProfile, isProfileLoading, status, updateProfileStatus} = props;\n\n    const onTextChange = (evt) => {\n        const text = evt.target.value;\n        onUpdatePostText(text);\n    };\n\n    const onSendButtonClick = () => {\n        addPost();\n    };\n\n    return (\n        <div className=\"content\">\n            {isProfileLoading ? <Loader/> :<ProfileInformation currentProfile={currentProfile} status={status} updateProfileStatus={updateProfileStatus}/>}\n            <div className=\"posts\">\n                <h2 className=\"posts__title\">\n                    My posts\n                </h2>\n                <div className=\"posts__new\">\n                    <textarea\n                        value={newPostText}\n                        onChange={onTextChange}\n                        type=\"text\"\n                        className=\"posts__input\"\n                        placeholder=\"Your new post...\"\n                    />\n                    <button\n                        onClick={onSendButtonClick}\n                        className=\"posts__button\"\n                    >Send</button>\n                </div>\n                <ul className=\"posts__list\">\n                    {posts.map((message, index) => {\n                        return <Posts key={index} text={message}/>\n                    }\n                    )}\n                </ul>\n            </div>\n        </div>\n    );\n};","/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/dialogs-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/profile-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/store.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/profile-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/dialogs-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/users-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/users.jsx",["125","126"],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/loader/loader.jsx",["127"],"import React from  'react';\nimport styles from './loader.css'\n\nexport const Loader = () => {\n    return (\n        <div className=\"loader\">Loading...</div>\n    )\n}","/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/auth-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/header-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/users-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/api/api.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/login.jsx",["128"],"import React from 'react';\nimport { Formik, Field, Form } from 'formik';\nimport {setLoginThunkCreator} from './../redux/auth-reducer';\nimport {connect} from 'react-redux';\nimport { Redirect } from 'react-router';\n\nexport const LoginComponent = (props) => {\n    const initialValues = {\n        login: '',\n        password: '',\n        rememberMe: false,\n    };\n\n    const validateLogin = (value) => {\n        let error;\n        if (!value) {\n            error = 'Required';\n        };\n        return error;\n    };\n\n    const validatePassword = (value) => {\n        let error;\n        if (!value) {\n            error = 'Required'\n        };\n        if (value.length < 8 && value) {\n            error = 'Should be more than 8 characters'\n        };\n        return error;\n    };\n\n    const onFormSubmit = (values, {setStatus}) => {\n        props.setLoginThunkCreator({email: values.login, password: values.password, rememberMe: values.rememberMe, setStatus});\n    };\n\n    if (props.isAuth) return <Redirect to={`/profile/${props.userId}`}/> \n\n    return (\n        <div className='login__wrapper'>\n            <h1 className='login__title'>Login</h1>\n            <Formik initialValues={initialValues} onSubmit={onFormSubmit}>\n                {({errors, touched, isValidating, status}) => (\n                    <Form className='login__input-wrapper'>\n                        <div className=\"login__input-container\">\n                            <label className='login__label' htmlFor='login'>Login</label>\n                            <Field className='login__input' id='login' name='login' type='text' validate={validateLogin} placeholder='login' validate={validateLogin} />\n                            {errors.login && touched.login && <div className='login__error'>{errors.login}</div>}\n                        </div>\n                        <div className=\"login__input-container\">\n                            <label className='login__label' htmlFor='password'>Password</label>\n                            <Field className='login__input' id='password' name='password' type='text' validate={validatePassword} placeholder='password' />\n                            {errors.password && touched.password && <div className='login__error'>{errors.password}</div>}\n                        </div>\n                        <div className=\"login__input-container\">\n                            <label className='login__label login__label--remember' htmlFor='rememberMe'>Remember me</label>\n                            <Field className='login__input--remember' id='rememberMe' name='rememberMe' type='checkbox' />\n                        </div>\n                        {status && <span className='login__error'>{status}</span>}\n                        <button className='login__button'>Submit</button>\n                    </Form>\n                )}\n            </Formik>\n\n        </div>\n    );\n};\n\nconst mapStateToProps = (state) => {\n    return {\n        isAuth: state.authData.isAuth,\n        userId: state.authData.id\n    }\n};\n\nexport const Login = connect(mapStateToProps, {setLoginThunkCreator})(LoginComponent);","/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/hocs/withAuthComponent.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/profile-status.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/app-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/users-selectors.js",["