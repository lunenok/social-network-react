nents/loader/loader.jsx",["115"],"import React from  'react';\nimport styles from './loader.css'\n\nexport const Loader = () => {\n    return (\n        <div className=\"loader\">Loading...</div>\n    )\n}","/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/redux/auth-reducer.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/header-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/users-container.jsx",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/api/api.js",[],"/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/components/login.jsx",["116"],"import React from 'react';\nimport { Formik, Field, Form } from 'formik';\nimport {setLoginThunkCreator} from './../redux/auth-reducer';\nimport {connect} from 'react-redux';\nimport { Redirect } from 'react-router';\n\nexport const LoginComponent = (props) => {\n    const initialValues = {\n        login: '',\n        password: '',\n        rememberMe: false,\n    };\n\n    const validateLogin = (value) => {\n        let error;\n        if (!value) {\n            error = 'Required';\n        };\n        return error;\n    };\n\n    const validatePassword = (value) => {\n        let error;\n        if (!value) {\n            error = 'Required'\n        };\n        if (value.length < 8 && value) {\n            error = 'Should be more than 8 characters'\n        };\n        return error;\n    };\n\n    const onFormSubmit = (values, {setStatus}) => {\n        props.setLoginThunkCreator({email: values.login, password: values.password, rememberMe: values.rememberMe, setStatus});\n    };\n\n    if (props.isAuth) return <Redirect to={`/profile/${props.userId}`}/> \n\n    return (\n        <div className='login__wrapper'>\n            <h1 className='login__title'>Login</h1>\n            <Formik initialValues={initialValues} onSubmit={onFormSubmit}>\n                {({errors, touched, isValidating, status}) => (\n                    <Form className='login__input-wrapper'>\n                        <div className=\"login__input-container\">\n                            <label className='login__label' htmlFor='login'>Login</label>\n                            <Field className='login__input' id='login' name='login' type='text' validate={validateLogin} placeholder='login' validate={validateLogin} />\n                            {errors.login && touched.login && <div className='login__error'>{errors.login}</div>}\n                        </div>\n                        <div className=\"login__input-container\">\n                            <label className='login__label' htmlFor='password'>Password</label>\n                            <Field className='login__input' id='password' name='password' type='text' validate={validatePassword} placeholder='password' />\n                            {errors.password && touched.password && <div className='login__error'>{errors.password}</div>}\n                        </div>\n                        <div className=\"login__input-container\">\n                            <label className='login__label login__label--remember' htmlFor='rememberMe'>Remember me</label>\n                            <Field className='login__input--remember' id='rememberMe' name='rememberMe' type='checkbox' />\n                        </div>\n                        {status && <span className='login__error'>{status}</span>}\n                        <button className='login__button'>Submit</button>\n                    </Form>\n                )}\n            </Formik>\n\n        </div>\n    );\n};\n\nconst mapStateToProps = (state) => {\n    return {\n        isAuth: state.authData.isAuth,\n        userId: state.authData.id\n    }\n};\n\nexport const Login = connect(mapStateToProps, {setLoginThunkCreator})(LoginComponent);","/Users/maksimditenberg/Documents/GitHub/finance-app/finance-app/src/hocs/withAuthComponent.jsx",[],"/Users/maksimditenberg